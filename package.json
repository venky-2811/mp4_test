{
  "name": "mp4_test",
  "version": "1.0.0",
  "description": "An Electron application for playing dashcam videos",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-packager . dashcamapp --platform=win32 --arch=x64 --icon=assets/icons/app-icon.ico --overwrite",
    "build:mac": "electron-packager . dashcamapp --platform=darwin --arch=x64 --icon=assets/icons/app-icon.icns",
    "build:linux": "electron-packager . dashcamapp --platform=linux --arch=x64 --icon=assets/icons/app-icon.png"
  },
  "dependencies": {
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^10.1.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "electron": "^26.6.10",
    "electron-packager": "^15.0.0"
  },
  "author": "venky",
  "license": "MIT"
}
